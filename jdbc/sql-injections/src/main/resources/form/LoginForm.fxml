<AnchorPane fx:id="root" prefHeight="213.0" prefWidth="366.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep11.app.controller.LoginFormController">
   <children>
      <StackPane layoutX="239.0" layoutY="14.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label text="Login">
               <font>
                  <Font name="Ubuntu Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </StackPane>
      <VBox alignment="CENTER" layoutX="61.0" layoutY="70.0" spacing="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0">
         <children>
            <Label text="Username" />
            <TextField fx:id="txtUsername" alignment="CENTER" promptText="Enter your username here" />
            <Label layoutX="10.0" layoutY="10.0" text="Password" />
            <PasswordField fx:id="txtPassword" alignment="CENTER" promptText="Enter your password here" />
            <Button fx:id="btnLogin" defaultButton="true" mnemonicParsing="false" onAction="#btnLoginOnAction" text="LOGIN" />
         </children>
      </VBox>
   </children>
